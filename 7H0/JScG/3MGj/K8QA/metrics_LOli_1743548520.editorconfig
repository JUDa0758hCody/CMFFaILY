# EditorConfig is awesome: https://EditorConfig.org


# Unix-style newlines with a newline ending every file
insert_final_newline = true
# Matches multiple files with brace expansion notation
# Set default charset
[*.{js,py}]
charset = utf-8

indent_style = space
indent_size = 4

[Makefile]
indent_style = tab

indent_size = 2
indent_style = space
indent_size = 2